{
    "manifest_version": 3,
    "name": "Social Media Blocker",
    "version": "0.0.1",
    "actions": {
        "default_title": "",
        "default_icon": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"   
        }
    },
    "icons": {
        "16": "images/icon.png",
        "32": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"   
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "Content.js"
            ]
        }
    ]
}